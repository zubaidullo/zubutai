{
  "name": "SubutaiTestBlueprint",
  "description": "Environment configuration test Subutai blueprint",
  "nodes": [
    {
      "name": "backup-server1",
      "template": "ubuntu16",
      "peer_group": "HTTP-GROUP",
      "size": "TINY"
    }
  ],
  "peerGroups": [
    {
      "name": "HTTP-GROUP",
      "pricingPref": {
        "maxPrice": "500"
      }
    }
  ],
  "sshKey": "titan",
  "configuration": {
    "arguments": [
      {
        "name" : "path",
        "value" : "/root"
      }
    ],
    "playbook": "https://github.com/zubaidullo/zubutai/archive/master.zip",
    "script": "main.yml",
    "groups": [
      {
        "name": "backups",
        "nodes_name": [
          "backup-server1"
        ]
      }
    ]
  }
}
