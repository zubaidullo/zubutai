{
  "name": "SubutaiTestBlueprint", //Environment name
  "description": "Environment configuration test Subutai blueprint",
  "nodes": [
    {
      "name": "backup-server1", //Container name
      "template": "ubuntu16", //Container template
      "peer_group": "HTTP-GROUP",
      "size": "TINY"
    }
  ],
  "peerGroups": [
    {
      "name": "HTTP-GROUP",
      "pricingPref": {
        "maxPrice": "500" //Subutai Goodwill
      }
    }
  ],
  "sshKey": "titan", //Subutai variables
  "configuration": {
    "arguments": [
      {
        "name" : "path",
        "value" : "/root"
      }
    ],
    "playbook": "https://github.com/zubaidullo/zubutai/archive/master.zip", //Repository archive file
    "script": "main.yml", //script file name
    "groups": [
      {
        "name": "backups", //name of container groups, where ansible script will be ran
        "nodes_name": [
          "backup-server1" //container name, to include to the group
        ]
      }
    ]
  }
}
